RewriteEngine On
    RewriteBase /
    # request to index.php
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php/$1 [L]
Header always set Content-Security-Policy "upgrade-insecure-requests;"

